{
  "name": "@doseofted/prim-docs",
  "version": "0.0.1",
  "description": "UI components for Prim documentation",
  "license": "UNLICENSED",
  "author": {
    "name": "Ted Klingenberg",
    "email": "ted@doseofted.com",
    "url": "https://doseofted.com/"
  },
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "check": "vue-tsc --noEmit",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint-style": "stylelint '**/*.{css,scss,vue}'",
    "test": "vitest run",
    "testing": "vitest watch"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "^2.0.0",
    "jsdom": "^20.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vite-plugin-dts": "^1.4.0",
    "vue-tsc": "^0.38.9"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.es.mjs",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.es.mjs",
      "require": "./dist/lib.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  }
}