{
  "name": "@doseofted/prim-frontend",
  "version": "0.0.1",
  "description": "An example frontend making use of Prim.",
  "license": "UNLICENSED",
  "author": {
    "name": "Ted Klingenberg",
    "email": "ted@doseofted.com",
    "url": "https://doseofted.com/"
  },
  "scripts": {
    "build": "vite build",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry.server.ts --target=esnext --outDir dist/server",
    "watch": "vite build --watch",
    "check": "vue-tsc --noEmit",
    "dev": "vite",
    "preview": "vite preview",
    "preview-test": "node server.mjs",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint-style": "stylelint '**/*.{css,scss,vue}'",
    "test": "vitest"
  },
  "dependencies": {
    "@doseofted/prim-docs": "workspace:*",
    "@doseofted/prim-example": "workspace:*",
    "@doseofted/prim-rpc": "workspace:*",
    "@fastify/middie": "^8.0.0",
    "fastify": "^4.0.3",
    "serve-static": "^1.15.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "^2.0.0",
    "jsdom": "^20.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vue-router": "^4.1.2",
    "vue-tsc": "^0.38.9"
  }
}