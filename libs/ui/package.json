{
  "name": "@doseofted/prim-ui",
  "version": "0.0.1",
  "description": "UI components for Prim documentation",
  "license": "UNLICENSED",
  "author": {
    "name": "Ted Klingenberg",
    "email": "ted@doseofted.com",
    "url": "https://doseofted.com/"
  },
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "check": "tsc --noEmit",
    "dev": "vite",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint-style": "stylelint '**/*.{css,scss}'",
    "test": "vitest run",
    "testing": "vitest watch"
  },
  "devDependencies": {
    "@doseofted/prim-docs": "workspace:*",
    "@doseofted/prim-example": "workspace:*",
    "@solidjs/router": "^0.4.3",
    "@tweakpane/core": "^1.1.0",
    "@tweakpane/plugin-essentials": "^0.1.5",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.16",
    "@unocss/reset": "^0.45.18",
    "lodash-es": "^4.17.21",
    "solid-js": "^1.5.4",
    "solid-styled-components": "^0.28.4",
    "solid-testing-library": "^0.3.0",
    "tweakpane": "^3.1.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.7.4",
    "unocss": "^0.45.18",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.5.0",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-solid": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.es.mjs",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.es.mjs",
      "require": "./dist/lib.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "dependencies": {
    "@motionone/solid": "^10.14.2",
    "color2k": "^2.0.0",
    "nanoid": "^4.0.0",
    "popmotion": "^11.0.5",
    "pts": "^0.11.1"
  }
}