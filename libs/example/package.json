  },
  "scripts": {
    "watch": "tsup --watch --config build.config.mjs",
    "build": "tsup --config build.config.mjs",
    "check": "tsc --noEmit",
    "document": "typedoc src/index.ts --json dist/docs.json",
    "test": "vitest run",
